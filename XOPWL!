-- KEY | HWID
 
_G.Key = "XOP-TIGER"
 
 
HWID = "1"
 
local Server =  syn.request({
    Url = "http://127.0.0.1/WHITELISTXOP/Server.php?Key=".. _G.Key .."&HWID="..HWID,
    Method = "GET"
}).Body
 
if Server == "WHITELIST !" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/fepfaaa/XOP-HUB-V.0.1.2/main/XOP-X.lua"))()
elseif Server == "Invaid HWID !" then
    game.Players.LocalPlayer:kick("Invaid HWID")
elseif Server == "Invaid Key" then
    game.Players.LocalPlayer:kick("Invaid Key")
else
    game.Players.LocalPlayer:kick("Invaid Key")
end
